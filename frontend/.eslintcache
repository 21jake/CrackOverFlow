t = e.map(element => element.value);\n      setTopics(output);\n    } else {\n      setTopics([]);\n    }\n  }\n\n\n  return (\n    <Container component=\"main\" maxWidth=\"xs\">\n      <CssBaseline />\n      <div className={classes.paper}>\n        <Avatar className={classes.avatar}>\n          {/* <LockOutlinedIcon /> */}\n        </Avatar>\n        <Typography component=\"h1\" variant=\"h5\">\n          Đăng ký\n        </Typography>\n        <AvForm onSubmit={register}>\n          <Grid container spacing={2}>\n            <Grid item xs={6}>\n              <AvField\n                name=\"fname\"\n                id=\"fname\"\n                type=\"text\"\n                label=\"Tên\"\n                autoFocus\n                validate={{\n                  required: { value: true, errorMessage: 'Vui lòng nhập tên' },\n                  maxLength: { value: 10, errorMessage: 'Tên không quá 10 ký tự' }\n                }}\n              />\n            </Grid>\n            <Grid item xs={6}>\n              <AvField\n                name=\"lname\"\n                id=\"lname\"\n                label=\"Họ\"\n                type=\"text\"\n                validate={{\n                  required: { value: true, errorMessage: 'Vui lòng nhập họ' },\n                  maxLength: { value: 10, errorMessage: 'Họ không quá 10 ký tự' },\n                }}\n              />\n\n            </Grid>\n            <Grid item xs={12}>\n              <TopicsDropdown \n              onTopicsChange={onTopicsChange}\n              isMultiple={true}\n              />\n            </Grid>\n            <Grid item xs={12}>\n              <AvField\n                name=\"email\"\n                id=\"email\"\n                label=\"Email\"\n                type=\"email\"\n                validate={{\n                  required: { value: true, errorMessage: 'Vui lòng nhập email' },\n                }}\n              />\n            </Grid>\n            <Grid item xs={12}>\n              <